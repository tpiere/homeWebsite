<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<!-- Servlets -->
	<servlet>
		<servlet-name>home</servlet-name>
		<jsp-file>/jsp/completeHtmlPages/home_complete.jsp</jsp-file>
		<init-param>
			<param-name>pageName</param-name>
			<param-value>home</param-value>
		</init-param>
	</servlet>

	<servlet>
		<servlet-name>dowJones</servlet-name>
		<jsp-file>/jsp/completeHtmlPages/dowJones_complete.jsp</jsp-file>
		<init-param>
			<param-name>pageName</param-name>
			<param-value>dowJones</param-value>
		</init-param>
	</servlet>
	
	<servlet>
		<servlet-name>thomsonReuters</servlet-name>
		<jsp-file>/jsp/completeHtmlPages/thomsonReuters_complete.jsp</jsp-file>
		<init-param>
			<param-name>pageName</param-name>
			<param-value>thomsonReuters</param-value>
		</init-param>
	</servlet>
	
	<servlet>
		<servlet-name>kustomJoes</servlet-name>
		<jsp-file>/jsp/completeHtmlPages/kustomJoes_complete.jsp</jsp-file>
		<init-param>
			<param-name>pageName</param-name>
			<param-value>kustomJoes</param-value>
		</init-param>
	</servlet>

	<servlet-mapping>
		<servlet-name>home</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>dowJones</servlet-name>
		<url-pattern>/dowJones</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>thomsonReuters</servlet-name>
		<url-pattern>/thomsonReuters</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>kustomJoes</servlet-name>
		<url-pattern>/kustomJoes</url-pattern>
	</servlet-mapping>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

</web-app>
